class Solution {
    public void moveZeroes(int[] nums) {
        int size = 0; 
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] != 0) {
                nums[size] = nums[i]; 
                size++; 
            }
        }
        for (int i = size; i < nums.length; i++) {
            nums[i] = 0;
        }
        
    }
}
