class Solution {
    public int strStr(String haystack, String needle) {
        
        int p=haystack.length();
        int s=needle.length();
        if(p==1 && haystack.equals(needle))
        return 0;
        for(int i=0;i<=p-s;i++){
            if(haystack.substring(i,i+s).equals(needle)){
                return i;
            }
        }
        return -1;
    }
}
